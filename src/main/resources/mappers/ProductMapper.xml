<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ks.poterrycafe.repository.GetProductMybatisRepository">

    <select id="findProductIdAndProductNameAndDescriptionAndQuantityAndPrice" resultType="ks.poterrycafe.dto.response.SaveProductResponse">
        SELECT product_id,
               product_name,
               description,
               quantity,
               price
        FROM product

    </select>
</mapper>